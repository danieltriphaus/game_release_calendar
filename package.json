    "cypress": "^10.3.0",
    "eslint": "^8.57.1",
    "eslint-plugin-cypress": "^2.15.2",
    "eslint-plugin-sonarjs": "^0.14.0",
    "eslint-plugin-vue": "^8.7.1",
    "isomorphic-fetch": "^3.0.0",
    "nodemon": "^3.1.9",
