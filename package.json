    "@popperjs/core": "^2.11.8",
    "@vitejs/plugin-vue": "^5.2.1",
    "axios": "^0.26.1",
    "body-parser": "^1.20.3",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.11.3",
    "bootstrap-vue-3": "^0.5.1",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.7",
    "core-js": "^3.39.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "express-xss-sanitizer": "^1.2.1",
    "google-auth-library": "^8.9.0",
    "ical-generator": "^3.6.1",
    "multer": "^1.4.4",
    "nanoid": "^3.3.8",
    "openapi-backend": "^5.11.1",
    "passport": "^0.7.0",
    "passport-google-oidc": "^0.1.0",
    "vite": "^6.0.5",
    "vue": "^3.5.13",
    "vue-debounce": "^5.0.1",
