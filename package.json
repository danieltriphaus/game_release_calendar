{
  "name": "game_release_calendar",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:event_pages": "vite ./src/event_pages --host",
    "dev:landingpages": "cd ./src/landingpages && npx @11ty/eleventy --serve",
    "build:landingpages": "cd ./src/landingpages && npx @11ty/eleventy",
    "build": "vite build",
    "build:event_pages": "vite build ./src/event_pages",
    "build:landingpage": "vite build ./src/landingpage",
    "server": "vite preview",
    "test:unit": "jest --runInBand",
    "test:cypressOpen": "yarn run cypress open --project tests",
    "test:cypress": "yarn run cypress run --project tests",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "startServer": "nodemon ./src/api/app.js",
    "start": "node src/api/app.js"
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@google-cloud/connect-datastore": "^7.0.1",
